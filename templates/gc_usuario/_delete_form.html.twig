<form method="post" action="{{ path('gc_usuario_delete', {'usId': gc_usuario.usId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ gc_usuario.usId) }}">
    <button class="btn">Delete</button>
</form>
