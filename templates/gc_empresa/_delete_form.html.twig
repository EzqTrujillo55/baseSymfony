<form method="post" action="{{ path('gc_empresa_delete', {'emId': gc_empresa.emId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ gc_empresa.emId) }}">
    <button class="btn">Delete</button>
</form>
